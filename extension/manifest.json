{
  "name": "Rayn",
  "description": "Rayn Extension.",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["node_modules/socket.io-client/dist/socket.io.min.js", "content.js"]
    }
  ],
  "_icons": {
    "16": "icons/png/16x16.png",
    "48": "icons/png/48x48.png",
    "128": "icons/png/128x128.png"
  }
}
